{
  "name": "jurchiks/commons",
  "type": "library",
  "description": "A collection of common tools",
  "keywords": [
    "utility",
    "common",
    "tools",
    "php7"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4.0"
  },
  "require-dev": {
    "ext-json": "*",
    "ext-intl": "*",
    "ext-fileinfo": "*",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.9.18"
  },
  "autoload": {
    "psr-4": {
      "js\\tools\\commons\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "js\\tools\\commons\\tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "qc": "rector process --dry-run"
  }
}
